[
//    { "key": "cmd+`",               "command": "workbench.action.terminal.toggleTerminal" },
//    { "key": "alt+cmd+left",      "command": "workbench.action.navigateBack" },
//    { "key": "alt+cmd+right",     "command": "workbench.action.navigateForward" },

    { "key": "alt+left",        "command": "workbench.action.navigateBack" },
    { "key": "alt+right",       "command": "workbench.action.navigateForward" },
    { "key": "ctrl+d",          "command": "editor.action.deleteLines", "when": "editorTextFocus" },
    { "key": "ctrl+shift+up",   "command": "editor.action.copyLinesUpAction", "when": "editorTextFocus" },
    { "key": "ctrl+shift+down", "command": "editor.action.copyLinesDownAction", "when": "editorTextFocus" },
    { "key": "ctrl+shift+r",    "command": "workbench.files.action.showActiveFileInExplorer" }
]
